<!doctype html>
<html>

<head>
	<title>Authentication Required</title>

	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width">
	<style type="text/css">
	html, body {
		margin: 0;
		padding: 0;
	}
	body {
		background-color: rgba(0, 0, 0, 0.6);
	}
	.container {
		display: grid;
		grid-template-rows: 15vh;
		grid-gap: 0px 0px;
	}
	.container > * {
		justify-self: center;
		align-self: center;
	}
	form > button, form > input {
		margin: 10px;
		padding: 5px;
	}
	form > button {
		text-align: center;
		min-width: 100px;
	}
	</style>
</head>

<body>
	<section class="container">
		<img src="data:image/png;base64," />
		{{#if error}}
			<p><strong>Error:</strong> {{error}}</p>
		{{/if}}
		<form method="post" action="/login">
			<input type="hidden" name="redirect" value="{{redirect}}" />
			<input type="hidden" name="host" value="{{host}}" />
			<input type="text" name="user" placeholder="User" tabindex="1" autofocus /><br />
			<input type="password" name="pass" placeholder="Passphrase" tabindex="2" /><br />
			<button type="submit">Login</button>
		</form>
	</section>
</body>

</html>
